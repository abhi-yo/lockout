{
  "expo": {
    "name": "Lockout",
    "slug": "lockout",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "lockout",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/MonaSans-Bold.otf",
            "./assets/fonts/MonaSans-Medium.otf",
            "./assets/fonts/MonaSans-SemiBold.otf"
          ]
        }
      ],
      "expo-secure-store",
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
